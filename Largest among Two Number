#include<stdio.h>
int main(){
    int x,y;
    printf("Enter the value of x and y ");
    scanf("%d%d", &x, &y);
    (x<y) ? printf("y is largest") : printf("x is largest");
    return 0;
}

\\2nd way 

#include<stdio.h>
int main(){
    int x,y;
    printf("Enter the value of x and y ");
    scanf("%d%d",&x,&y);
    if(x<y)
    printf("y is largest");
else
    printf("x is largest");
    return 0;
}
